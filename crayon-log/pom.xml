    <artifactId>crayon-log</artifactId>

    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-aop</artifactId>
        </dependency>
        <dependency>
            <groupId>com.crayon</groupId>
            <artifactId>crayon-common</artifactId>
        </dependency>
        <dependency>
